@page
@model DigitalHealthCheckWeb.Pages.FollowUpActionsModel
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Next steps – Actions";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.FollowUpActionsPage)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-selectedInterventions="@Model.SelectedInterventions"
           param-Interventions="@Model.AllInterventions"
           param-customBarriers="@Model.CustomBarriers" 
           param-actionCategories="@Model.ActionCategories"/>